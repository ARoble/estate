  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, maximum-scale=1"
    />

    <title>Resido - Residence & Real Estate HTML Template</title>

    <!-- Custom CSS -->
    <link href="public/css/styles.css" rel="stylesheet" />

    <!-- Custom Color Option -->
    <link href="public/css/colors.css" rel="stylesheet" />
  </head>

  <body class="blue-skin">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div id="preloader">
      <div class="preloader"><span></span><span></span></div>
    </div>

    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">
      <!-- ============================================================== -->
      <!-- Top header  -->
      <!-- ============================================================== -->
      <%- include('partials/header') %>
      <div class="clearfix"></div>
      <!-- ============================================================== -->
      <!-- Top header  -->
      <!-- ============================================================== -->

      <!-- ============================ Page Title Start================================== -->
      <div class="page-title">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 col-md-12">
              <h2 class="ipt-title">Submit Property</h2>
              <span class="ipn-subtitle">Just Submit Your Property</span>
            </div>
          </div>
        </div>
      </div>
      <!-- ============================ Page Title End ================================== -->

      <!-- ============================ Submit Property Start ================================== -->
      <section class="gray-simple">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 col-md-12">
              <div class="alert alert-success" role="alert">
                <p>
                  Please, Sign In before you submit a property. If you don't
                  have an account you can create one by
                  <a href="#">Clicking Here</a>
                </p>
              </div>
            </div>
			
			  
			  
            <!-- Submit Form -->
            <div class="col-lg-12 col-md-12">
				<form action="/add-property" method="POST" enctype="multipart/form-data">
              <div class="submit-page">
                <!-- Basic Information -->
                <div class="form-submit">
                  <h3>Basic Information</h3>
                  <div class="submit-section">
                    <div class="row">
                      <div class="form-group col-md-12" >
                        <label
                          >Property Title<span
                            class="tip-topdata"
                            data-tip="Property Title"
                            ><i class="ti-help"></i></span
                        ></label>
                        <input type="text" class="form-control" name="title"/>
                      </div>

                      <div class="form-group col-md-6" id="property-form">
                        <label>Property Type</label>
                        <select id="ptypes" class="form-control" name="propertyType" >
                          <option value="">&nbsp;</option>
                          <option value="House">House</option>
                          <option value="Office">Office</option>
                          <option value="Land">Land</option>
                        </select>
                      </div>

					  <div class="form-group col-md-6" id="yeah">
                        <label>Status</label>
                        <select id="status" class="form-control" name="status">
                          <option value="">&nbsp;</option>
                          <option value="rent">For Rent</option>
                          <option value="sale">For Sale</option>
                        </select>
                      </div>

                      <div class="form-group col-md-6" >
                        <label>Price</label>
                        <input
                          type="text"
                          class="form-control"
                          placeholder="USD"
						  name="price"
                        />
                      </div>

                      <div class="form-group col-md-6">
                        <label>Area</label>
                        <input type="text" class="form-control" name="area"/>
                      </div>

					  <div class="form-group col-md-6" id="bed">
                        <label>Bedrooms</label>
                        <input type="number" class="form-control" name="bedrooms"/>
                      </div>

					  <div class="form-group col-md-6" id="bath">
                        <label>Bathrooms</label>
                        <input type="number" class="form-control" name="bathrooms"/>
                      </div>

					  <div class="form-group col-md-6" >
                        <label>imageCover</label>
                        <input type="file" id="myfile" name="imageCover" >
                      </div>
					  <div class="form-group col-md-6" >
                        <label>Images</label>
                        <input type="file" id="myfile" name="images" multiple>
                      </div>

                    </div>
                  </div>
                </div>

                <!-- Gallery -->
                <div class="form-submit">
                  <h3>Gallery</h3>
                  <div class="submit-section">
                    <div class="row">
                      <div class="form-group col-md-12">
                        <label>Upload Gallery</label>
                       
                          <div class="dz-default dz-message">
                            <i class="ti-gallery"></i>
                            <span>Drag & Drop To Change Logo</span>
                          </div>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- Location -->
                <div class="form-submit">
                  <h3>Location</h3>
                  <div class="submit-section">
                    <div class="row">
                      <div class="form-group col-md-6">
                        <label>Address</label>
                        <input type="text" class="form-control" name="address" />
                      </div>

                      <div class="form-group col-md-6">
                        <label>Neighbourhood</label>
                        <input type="text" class="form-control" name="neighbourhood"/>
                      </div>

                      <div class="form-group col-md-6">
                        <label>City</label>
                        <select id="ptypes" class="form-control" name="city">
							<option value="">&nbsp;</option>
							<option value="Hargeisa">Hargeisa</option>
							<option value="Burco">Burco</option>
							<option value="Berbera">Berbera</option>
							<option value="Laas Anood">Laas Anood</option>
							<option value="Ceerigaabo">Ceerigaabo</option>
							<option value="Bosaaso">Bosaaso</option>
						  </select>
                      </div>

                    </div>
                  </div>
                </div>

                <!-- Detailed Information -->
                <div class="form-submit">
                  <h3>Detailed Information</h3>
                  <div class="submit-section">
                    <div class="row">
                      <div class="form-group col-md-12">
                        <label>Description</label>
                        <textarea class="form-control h-120" name="description"></textarea>
                      </div>

                     

                      <div class="form-group col-md-12">
                        <label>Other Features (optional)</label>
                        <div class="o-features">
                          <ul class="no-ul-list third-row">
                            <li>
                              <input
                                id="a-1"
                                class="checkbox-custom"
                                name="water"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-1" class="checkbox-custom-label"
                                >Water</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-2"
                                class="checkbox-custom"
                                name="garage"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-2" class="checkbox-custom-label"
                                >Garage</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-3"
                                class="checkbox-custom"
                                name="tiles"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-3" class="checkbox-custom-label"
                                >Tiles</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-4"
                                class="checkbox-custom"
                                name="furnished"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-4" class="checkbox-custom-label"
                                >Furnished</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-5"
                                class="checkbox-custom"
                                name="kitchenOutside"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-5" class="checkbox-custom-label"
                                >Kitchen Outside</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-6"
                                class="checkbox-custom"
                                name="a-6"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-6" class="checkbox-custom-label"
                                >Smoking Allow</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-7"
                                class="checkbox-custom"
                                name="a-7"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-7" class="checkbox-custom-label"
                                >Terrace</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-8"
                                class="checkbox-custom"
                                name="a-8"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-8" class="checkbox-custom-label"
                                >Balcony</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-9"
                                class="checkbox-custom"
                                name="a-9"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-9" class="checkbox-custom-label"
                                >Icon</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-10"
                                class="checkbox-custom"
                                name="a-10"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-10" class="checkbox-custom-label"
                                >Wi-Fi</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-11"
                                class="checkbox-custom"
                                name="a-11"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-11" class="checkbox-custom-label"
                                >Beach</label
                              >
                            </li>
                            <li>
                              <input
                                id="a-12"
                                class="checkbox-custom"
                                name="a-12"
								value="true"
                                type="checkbox"
                              />
                              <label for="a-12" class="checkbox-custom-label"
                                >Parking</label
                              >
                            </li>
                          </ul>
                        </div>
                      </div>
					  <div class="form-group col-lg-12 col-md-12">
						<button class="btn btn-theme-light-2 rounded" type="submit">
						  Submit & Preview
						</button>
					  </div>
                    </div>
                  </div>
                </div>

              
        </div>
	</form>
			</div>
      </section>
      <!-- ============================ Submit Property End ================================== -->

     

      <!-- ============================ Footer Start ================================== -->
      <%- include('partials/footer') %>
      <!-- ============================ Footer End ================================== -->

      <a id="back2Top" class="top-scroll" title="Back to top" href="#"
        ><i class="ti-arrow-up"></i
      ></a>
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->

    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="public/js/jquery.min.js"></script>
    <script src="public/js/popper.min.js"></script>
    <script src="public/js/bootstrap.min.js"></script>
    <script src="public/js/rangeslider.js"></script>
    <script src="public/js/select2.min.js"></script>
    <script src="public/js/jquery.magnific-popup.min.js"></script>
    <script src="public/js/slick.js"></script>
    <script src="public/js/slider-bg.js"></script>
    <script src="public/js/lightbox.js"></script>
    <script src="public/js/imagesloaded.js"></script>

    <script src="public/js/custom.js"></script>
	<script src="public/js/script.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->

    <script src="public/js/dropzone.js"></script>
    <!-- New Js -->
  </body>

